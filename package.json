{
  "name": "api-bke-express",
  "version": "1.0.0",
  "description": "api node com express para backend do terceiro semestre",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "node --env-file=.env --watch src/server.js",
    "prisma:pull": "npx prisma db pull --schema=./prisma/schema-production.prisma",
    "prisma:push": "npx prisma db push --schema=./prisma/schema-production.prisma",
    "prisma:generate": "npx prisma generate --schema=./prisma/schema-production.prisma",
    "prisma:local:pull": "npx prisma db pull --schema=./prisma/schema-local.prisma",
    "prisma:local:push": "npx prisma db push --schema=./prisma/schema-local.prisma",
    "prisma:local:generate": "npx prisma generate --schema=./prisma/schema-local.prisma",
    "prisma:local:studio": "npx prisma db pull --schema=./prisma/schema-local.prisma"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GArci4rock/apibkeexpress.git"
  },
  "keywords": [
    "api",
    "express",
    "zod",
    "mvc"
  ],
  "author": "Matheus Garcia",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GArci4rock/apibkeexpress/issues"
  },
  "homepage": "https://github.com/renancavichi/api-bke-express-2sem2024#readme",
  "dependencies": {
    "@prisma/client": "^5.19.0",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "prisma": "^5.19.0"
  }
}
